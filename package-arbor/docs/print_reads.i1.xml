<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>print_reads</name>
  		<command>
    		<type>RunUnixCommand</type>
    		<name>replace_config_keys</name>
    		<state>incomplete</state>
    		<executable>$;BIN_DIR$;/replace_config_keys</executable>
    		<param>  
      			<key>--template_conf</key>
      			<value>$;I_FILE_PATH$;</value>
    		</param>
    		<param>  
      			<key>--output_conf</key>
      			<value>$;I_FILE_PATH$;.final</value>
    		</param>   
    		<param>
      			<key>stdout</key>
	  			<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/replace_input_config_keys.stdout</value>
    		</param> 
    		<param>
      			<key>stderr</key>
	  			<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/replace_input_config_keys.stderr</value>
    		</param>
  		</command>
		<command>
            <type>RunUnixCommand</type>
            <name>print_reads</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/print_reads</executable>
            <param>
                <key>--config_file</key>
                <value>$;I_FILE_PATH$;.final</value>
            </param>
            <param>
                <key>--outdir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>--tmpdir</key>
                <value>$;TEMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.print_reads.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.print_reads.stderr</value>
            </param>
            <arg>$;OTHER_ARGS$;</arg>
        </command>
    </commandSet>
</commandSetRoot>
