<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>indel_realigner</name>
		<command>
            <type>RunUnixCommand</type>
            <name>indel_realigner</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/indel_realigner</executable>
            <param>
                <key>--config_file</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
                <key>--outdir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>--tmpdir</key>
                <value>$;TEMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.indel_realigner.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.indel_realigner.stderr</value>
            </param>
            <arg>$;OTHER_ARGS$;</arg>
        </command>
    </commandSet>
</commandSetRoot>
